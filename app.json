{
  "expo": {
    "name": "bpwallets",
    "slug": "777wallets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "777wallets",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zaheerahmad.777wallets",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library so you can upload a bank logo.",
        "NSCameraUsageDescription": "We need access to your camera so you can take a photo for your bank account logo."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zaheerahmad.x777wallets"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98967c5f-e76c-4a5b-a4e6-acdbe979f00c"
      }
    }
  }
}
